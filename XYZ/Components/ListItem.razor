@using Need4Protocol;
@using XYZ.Data;
@inject Need4Service Need4Service

<h3>ItemList</h3>


@if (list != null)
{
    <div>
    @foreach (Item i in list.Items)
    {
        <br />
        <span>Name: @i.Name </span>
    }
    </div>
}


@code {
    [Parameter]
    public ItemList list { get; set; }
}